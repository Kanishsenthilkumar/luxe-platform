{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "dev:ecommerce": "vite --port 3000",
    "dev:admin": "cd admin-panel && npm run dev",
    "dev:api": "cd server && npm run dev",
    "dev:all": "concurrently \"npm run dev:api\" \"npm run dev\" \"npm run dev:admin\"",
    "build": "vite build",
    "build:ecommerce": "vite build",
    "build:admin": "cd admin-panel && npm run build",
    "build:api": "cd server && npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "install:admin": "cd admin-panel && npm install",
    "install:api": "cd server && npm install",
    "setup": "npm install && npm run install:admin && npm run install:api"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "mysql2": "^3.14.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
